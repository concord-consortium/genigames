@import 'app/style/mixins/position'

#container
  absolute: width 100%
  overflow: hidden
  > div
    relative: width 1000px height 750px
    margin-left: auto
    margin-right: auto

town-style(img)
  background: top left no-repeat url(img)
  margin-left: auto
  margin-right: auto
  width: 1010px
  height: 660px

#town
  town-style: '../images/town01.jpg'

#town1
  town-style: '../images/town01.jpg'

#town2
  town-style: '../images/town02a.jpg'

