<div class="course">
  <div class="drake-container">
    {{#if view.drake}}
      {{view GG.DrakeView contentBinding="view.drake"}}
    {{/if}}
  </div>
  <div class="background"></div>
  <div class="obstacles">
    {{#each obstacle in view.obstacles}}
      {{view GG.ObstacleView typeBinding="obstacle"}}
    {{/each}}
  </div>
</div>
<div class="controls">
  <a href="#" {{action "start" view}}>Start</a>
  <a href="#" {{action "reset" view}}>Reset</a>
</div>
